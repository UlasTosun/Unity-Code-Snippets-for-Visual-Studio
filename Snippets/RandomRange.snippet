<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>RandomRange</Title>
            <Author>ULAS</Author>
            <Shortcut>uRandomR</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[Random.Range($min$, $max$);]]>
            </Code>
              <Declarations>
                <Literal>
                  <ID>min</ID>
                  <Default>min</Default>
                </Literal>
                <Literal>
                  <ID>max</ID>
                  <Default>max</Default>
                </Literal>
              </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>