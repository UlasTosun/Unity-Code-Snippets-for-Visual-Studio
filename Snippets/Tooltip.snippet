<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Tooltip</Title>
            <Author>ULAS</Author>
            <Shortcut>uTooltip</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[[Tooltip($description$)]$end$]]>
            </Code>
              <Declarations>
                <Literal>
                  <ID>description</ID>
                  <Default>description</Default>
                </Literal>
              </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>